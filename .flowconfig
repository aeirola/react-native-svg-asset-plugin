[ignore]
<PROJECT_ROOT>/example/.*

[include]

[untyped]
<PROJECT_ROOT>/node_modules/metro/src/shared/.*

[libs]

[lints]
all=warn
dynamic-export=off

[options]
include_warnings=true
module.ignore_non_literal_requires=true

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
